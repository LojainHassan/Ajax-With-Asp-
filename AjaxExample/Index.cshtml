@section Scripts {
   <script>
       $(document).ready(function(){

            $.ajax({
                type: "GET",
                url: "@Url.Action("GetNames")",
                datatype: "json",
                success: function (result) {
                    console.log(result);
                },
                error: function (req, status, error) {
                    console.log(status);
                    }

            });


            $.ajax({
                type: "Post",
                url: "@Url.Action("PostName")",
                datatype: "json",
                data :{name :"Lojain Hassan"},
                success: function (result) {
                    console.log(result);
                },
                error: function (req, status, error) {
                    console.log(status);
                }

            });



       });



   </script>
}
